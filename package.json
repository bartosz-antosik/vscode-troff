{
    "name": "troff",
    "displayName": "TROFF Syntax",
    "description": "TROFF Syntax Highlighter",
    "version": "1.0.20",
    "publisher": "ban",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "troff",
        "nroff",
        "groff"
    ],
    "contributes": {
        "languages": [
            {
                "id": "troff",
                "aliases": [
                    "Troff",
                    "troff"
                ],
                "extensions": [
                    ".troff",
                    ".nroff",
                    ".groff",
                    ".1",
                    ".2",
                    ".3",
                    ".4",
                    ".5",
                    ".6",
                    ".7",
                    ".8",
                    ".9"
                ],
                "configuration": "./troff.json"
            }
        ],
        "grammars": [
            {
                "language": "troff",
                "scopeName": "text.troff",
                "path": "./syntaxes/troff.tmLanguage"
            }
        ]
    },
    "icon": "icon.png",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://github.com/bartosz-antosik/vscode-troff",
    "repository": {
        "type": "git",
        "url": "https://github.com/bartosz-antosik/vscode-troff.git"
    },
    "bugs": {
        "url": "https://github.com/bartosz-antosik/vscode-troff/issues"
    },
